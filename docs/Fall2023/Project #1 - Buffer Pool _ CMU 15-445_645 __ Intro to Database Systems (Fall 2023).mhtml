mportant; }

.bg-danger a { color: white !important; }

.banner-title { background-color: rgba(0, 0, 0, 0.6); }

.sponsors img { max-width: 300px; }

.awards img { max-width: 125px; height: auto; }

.published { font-size: 90%; }

div.question { border-radius: 15px; background-color: rgb(237, 238, 239); p=
adding: 1.25rem 1.25rem 0.5rem; margin-bottom: 2rem; }

